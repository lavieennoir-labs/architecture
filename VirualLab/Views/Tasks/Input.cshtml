    <div id="interfaceWrapper" class="box horizontal flex1">
        <div id="controls" class="box vertical flex1 visBackgroundColor">
            <div id="commandLineHistory" class="box vertical flex3">
                <div class="toolbar box vertical center">
                    <div class="controls box horizontal justify">
                        <div class="box flex1">
                            <div class="close">
                            </div>
                        </div>
                        <div class="box flex1">
                            <div class="minimize">
                            </div>
                        </div>
                        <div class="box flex1">
                            <div class="plus">
                            </div>
                        </div>
                    </div>
                    <div>
                        <i class="icon-home"></i>
                        <span id="lesson-name" class="vcs-mode-aware intl-aware" data-intl="learn-git-branching">Вивчаємо розгалудження у GIT</span>
                    </div>
                </div>
                <div id="levelToolbarMount">
                    <div class="toolbar level-toolbar box vertical center transitionAll " data-reactid=".2">
                        <div class="clearfix" data-reactid=".2.0">
                            <div class="levelNameWrapper" data-reactid=".2.0.0">
                                <i class="icon-bolt" data-reactid=".2.0.0.0"></i>
                                <span data-reactid=".2.0.0.1"> Рівень </span>
                                <span class="levelToolbarSpan" data-reactid=".2.0.0.2">Знайомство з Git Commit </span>
                            </div>
                        </div>
                        <div style="margin-bottom:8px" data-reactid=".2.1">
                            <div class="showGoalWrapper" data-reactid=".2.1.0">
                                <button type="button" style="cursor:pointer; font-size: 1rem;" onclick="window.location.href='/Tasks/Goal'" data-reactid=".2.1.0.0">Ціль рівня</button>
                            </div>
                            <div class="showObjectiveWrapper" data-reactid=".2.1.1">
                                <button id="check-btn" disabled type="button" style="cursor:pointer; font-size: 1rem;" onclick="checkTask()" data-reactid=".2.1.1.0">Перевірити виконання</button>
                            </div>
                        </div>
                    </div>
                </div>

                <div id="terminal" class="box flex1 horizontal startAlign" onclick="document.getElementById('command-input').focus(); return false;">
                    <div id="commandDisplay" class="box vertical flex1">
                        <div data-reactid=".1">
                            <div id="command" class="reactCommandView" data-reactid=".1.$c45">
                                <p class="finished commandLine transitionBackground" data-reactid=".1.$c45.0"></p>
                                <div class="commandLineResult" data-reactid=".1.$c45.1">
                                    <p data-reactid=".1.$c45.1.$paragraph_0">
                                        Спробуй двічі виконати команду 'git commit'
                                    </p>
                                </div>
                                <div class="commandLineWarnings" data-reactid=".1.$c45.2">

                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div id="commandLineBar" class="box vertical flex0" onclick="document.getElementById('command-input').focus(); return false;">
                    <div id="prompt" class="">
                        <span class="promptSign box">$</span>
                        <span class="cursor box">&nbsp;</span>
                        <p class="command">
                            <form class="command-form">
                                <input type="text" id="command-input" style="all:unset;width: 90%;" />
                                <button type="submit" style="display:none"></button>
                            </form>
                            <span class="commandCursor"></span><span>					&nbsp;</span>
                        </p>
                    </div>
                    <textarea autocomplete="off" autocorrect="off" autocapitalize="off" spellcheck="false" id="commandTextField">					</textarea>
                </div>

            </div>
        </div>

        <div id="mainVisSpace" class="box flex2 horizontal transitionAll visBackgroundColor">
            <span class="modeText transitionOpacity gitMode">
                Git
            </span>
            <span class="modeText transitionOpacity hgMode">
                Hg
            </span>
        </div>

        <div id="goalPlaceholder" class="box flex1 visBackgroundColor" style="display: none;"></div>

    </div>

